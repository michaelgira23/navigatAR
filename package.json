{
  "name": "navigatar",
  "version": "1.0.0",
  "description": "Finding your way through buildings and campuses through the power of augmented reality",
  "private": true,
  "main": "./node_modules/react-native-scripts/build/bin/crna-entry.js",
  "scripts": {
    "start": "react-native-scripts start",
    "test": "node node_modules/jest/bin/jest.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/michaelgira23/navigatAR.git"
  },
  "keywords": [
    "navigatAR",
    "autmented-reality",
    "indoor-maps"
  ],
  "author": "MICDS WWT Hackathon Team",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/michaelgira23/navigatAR/issues"
  },
  "homepage": "https://github.com/michaelgira23/navigatAR#readme",
  "babel": {
    "presets": [
      "babel-preset-expo"
    ],
    "env": {
      "development": {
        "plugins": [
          "transform-react-jsx-source"
        ]
      }
    }
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "ios.ts",
      "ios.tsx",
      "android.ts",
      "android.tsx"
    ]
  },
  "dependencies": {
    "@expo/vector-icons": "^6.2.2",
    "expo": "^25.0.0",
    "firebase": "^4.9.0",
    "react": "^16.2.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-25.0.0.tar.gz",
    "react-navigation": "^1.0.0-beta.27",
    "tslib": "^1.8.0"
  },
  "devDependencies": {
    "@types/expo": "^24.0.3",
    "@types/jest": "^22.0.1",
    "@types/react": "^16.0.34",
    "@types/react-native": "^0.52.1",
    "@types/react-navigation": "^1.0.28",
    "@types/react-test-renderer": "^16.0.0",
    "jest-expo": "^25.0.0",
    "react-native-scripts": "^1.9.0",
    "react-native-typescript-transformer": "^1.2.2",
    "react-test-renderer": "^16.2.0",
    "ts-jest": "^22.0.1",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "*",
    "tslint-react": "^3.4.0",
    "typescript": "^2.6.2"
  }
}
